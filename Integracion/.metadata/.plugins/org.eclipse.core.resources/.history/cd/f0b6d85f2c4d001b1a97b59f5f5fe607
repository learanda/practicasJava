package ar.linkedlist;

import java.util.LinkedList;

public class Linky {

	public static void main(String[] args) {
		
		LinkedList linky = new LinkedList();
		
		linky.add("Lan");
		linky.add("Boris");
		linky.add("Hercu");
		
		linky.remove(0);
		
		System.out.println(linky.getFirst());
		
		System.out.println(linky);

	}

}
