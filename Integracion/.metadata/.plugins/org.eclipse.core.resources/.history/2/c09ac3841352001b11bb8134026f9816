package ar.thisisfun;

public class ThisIsFun {
	
	int a;
	int b;
	
	public static void setData(int a, int b) {
		this.a = a;
		this.b = b;
	}

	public static void main(String[] args) {
		
		setData(3, 4);
		
		ThisIsFun t = new ThisIsFun();
		
		System.out.println(t.a);
		System.out.println(t.b);

	}

}
