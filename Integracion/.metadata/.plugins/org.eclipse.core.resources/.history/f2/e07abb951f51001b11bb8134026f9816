package ar.oop;

public class Main {

	public static void main(String[] args) {
		
		Lapicera p = new Lapicera();
		
		System.out.println(p.color);
		System.out.println(p.tipo);
		System.out.println(p.punto);
		
		System.out.println(p.clickeada);
		
		p.click();
		
		System.out.println(p.clickeada);

	}

}
